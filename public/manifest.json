{
  "name": "Gemini Ext",
   "description": "Gemini cheese",
   "version": "1.0",
   "manifest_version": 3,
   "action": {
       "default_popup": "index.html",
       "default_title": "Open the popup"
   },
   "icons": {
       "16": "bot.png",
       "48": "bot.png",
       "128": "bot.png"
   },
   "background": {
    "service_worker": "background.js"
    },
   "permissions": [
    "storage", "audio", "runtime"
    ]
}
